				<DatabaseName>ACT_NAME</DatabaseName>
				<Label>Naziv</Label>
				<Type>String</Type>
				<Unique>true</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
			<attribute>
				<Name>link</Name>
				<DatabaseName>ACT_LINK</DatabaseName>
				<Label>Link</Label>
				<Type>String</Type>
				<Unique>false</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
			<attribute>
				<Name>imagePath</Name>
