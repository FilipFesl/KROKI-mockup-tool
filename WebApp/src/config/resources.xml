		<ManyToOneAttributes>
			<manyToOne>
				<Name>user</Name>
				<DatabaseName>MR_USER</DatabaseName>
				<Label>Korisnik</Label>
				<Type>User</Type>
				<Mandatory>true</Mandatory>
			</manyToOne>
