			<attribute>
				<Name>allowed</Name>
				<DatabaseName>UR_ALLOWED</DatabaseName>
				<Label>Dozvoljeno</Label>
				<Type>Boolean</Type>
				<Unique>false</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
		</Attributes>
		<ManyToOneAttributes>
			<manyToOne>
				<Name>user</Name>
				<DatabaseName>UR_USER</DatabaseName>
				<Label>Korisnik</Label>
				<Type>User</Type>
				<Mandatory>true</Mandatory>
			</manyToOne>
			<manyToOne>
				<Name>action</Name>
				<DatabaseName>UR_ACTION</DatabaseName>
				<Label>Akcija</Label>
				<Type>Action</Type>
				<Mandatory>true</Mandatory>
			</manyToOne>
