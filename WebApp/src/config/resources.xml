	<resource>
		<Name>Resource</Name>
		<Label>Resursi</Label>
		<Link>/resources/resursi</Link>
		<Routed>true</Routed>
		<Forms>StandardForm,ParentChildForm</Forms>
		<Attributes>
			<attribute>
				<Name>name</Name>
				<DatabaseName>RES_NAME</DatabaseName>
				<Label>Naziv</Label>
				<Type>String</Type>
				<Unique>true</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
			<attribute>
				<Name>link</Name>
				<DatabaseName>RES_LINK</DatabaseName>
				<Label>Adresa</Label>
				<Type>String</Type>
				<Unique>true</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
		</Attributes>
	</resource>
	<resource>
		<Name>User</Name>
		<Label>Korisnici</Label>
		<Link>/resources/korisnici</Link>
		<Routed>true</Routed>
		<Forms>StandardForm,ParentChildForm</Forms>
		<Attributes>
			<attribute>
				<Name>name</Name>
				<DatabaseName>US_USERNAME</DatabaseName>
				<Label>Korisni&#269;ko ime</Label>
				<Type>String</Type>
				<Unique>true</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
			<attribute>
				<Name>password</Name>
				<DatabaseName>US_PASSWORD</DatabaseName>
				<Label>Lozinka</Label>
				<Type>String</Type>
				<Unique>false</Unique>
				<Mandatory>true</Mandatory>
			</attribute>
		</Attributes>
		<OneToManyAttributes>
			<oneToMany>
				<Name>rights</Name>
				<Label>Prava</Label>
				<RefferencedTable>UserRights</RefferencedTable>
				<MappedBy>user</MappedBy>
			</oneToMany>
		</OneToManyAttributes>
	</resource>
	<resource>
		<Name>UserRights</Name>
